   -
     code : |1+
      // récupère le message d'erreur complet du problème rencontré
      gsDetail est une chaine = herreurinfo(hErrComplet)
      
      constant HAUTEURMAX=200
      
      // titre de la fenêtre = nom de l'application
      moimeme..Titre = fExtraitChemin(ExeInfo(exeNom),fFichier)
      // Valeur de retour par défaut si fermeture autrement que par les boutons
      MoiMême..ValeurRenvoyée=opANNULER
      
      // si il s'agit d'un blocage de la base de données
      si HErreur() = 74011 ALORS
      	BlocageSurBaseDeDonnées()
      sinon 
      	// il s'agit d'un blocage sur un fichier
      	BlocageSurFichier()
      FIN
      
      
