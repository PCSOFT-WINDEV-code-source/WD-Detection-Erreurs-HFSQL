  procedures :
   -
     name : BlocageSurFichier
     procedure_id : 1606746457989741117
     type_code : 14
     code : |1+
      Procédure BlocageSurFichier()
      
      i est un entier = 0
      
      // récupère les infos de blocage
      sInfoBlocage est une chaine = herreurinfo(hErrinfoBlocage)
      
      // si on a des infos sur le blocage
      si sInfoBlocage <> "" ALORS
      	// mise en forme du libellé
      	LIB_Explications = ChaîneConstruit(<§$0008§>,HErreurInfo(hErrFichier))	
      	
      	sMsg est une chaine
      	sUnBlocage est une chaine = ExtraitChaîne(sInfoBlocage, rangPremier, rc)
      	tantque sUnBlocage <> EOT
      		i++
      		sMsg += ChaîneConstruit(<§$0009§>+rc,ExtraitChaîne(sUnBlocage, 3, tab),ExtraitChaîne(sUnBlocage, 6, tab),ExtraitChaîne(sUnBlocage, 4, tab))
      		sUnBlocage = ExtraitChaîne(sInfoBlocage, rangsuivant, rc)
      	FIN
      	si droite(sMsg,2) = RC alors sMsg = sMsg[[ a taille(sMsg)-2 ]]
      	nHauteur est un entier
      
      	// si on a une seule ligne
      	si i=1 alors
      		// supprime le '- ' de devant
      		sMsg = sMsg[[ 3 a taille(sMsg) ]]
      		// règle la hauteur de la fenêtre en fonction des infos de blocage
      		nHauteur = TexteHauteur(ListeBlocages,sMsg,tMultiligne)
      		nHauteur = min(nHauteur,HAUTEURMAX)
      		MoiMême..hauteur += nHauteur
      
      		ListeBlocages = sMsg
      		ListeBlocages..visible = vrai
      	sinon
      		// règle la hauteur de la fenêtre en fonction des infos de blocage
      		nHauteur = TexteHauteur(ListeBlocagesMulti,sMsg)
      		nHauteur = min(nHauteur,HAUTEURMAX)
      		si nHauteur = HAUTEURMAX ALORS AscenseurAffiche(ListeBlocagesMulti,ascVert,vrai)
      		si TexteLargeur(ListeBlocagesMulti,sMsg)>ListeBlocagesMulti..Largeur alors
      			AscenseurAffiche(ListeBlocagesMulti,ascHorz,vrai)
      			// on rajoute la hauteur de l'ascenseur + 5 pixels pour que le texte ne soit pas collé à l'ascenseur
      			nHauteur = nHauteur+SysMétrique(metCyHScroll)+5
      		FIN
      		MoiMême..hauteur += nHauteur
      
      		ListeBlocagesMulti = sMsg
      		ListeBlocagesMulti..visible = vrai
      	FIN
      sinon	
      	// mise en forme du libellé
      	LIB_Explications = <§$0003§>+HErreurInfo(hErrFichier)+<§$0004§>
      FIN
     type : 458752
   -
     name : BlocageSurBaseDeDonnées
     procedure_id : 1606746457989806653
     type_code : 14
     code : |1+
      Procédure BlocageSurBaseDeDonnées()
      
      // préciser la nature du blocage
      entete = <§$0010§>
      
      // récupère les infos sur le blocage de la base
      sInfoBlocage est une chaine = herreurinfo(hErrInfoBlocage)
      
      // récupère le nom de la base
      sNomBase est une chaine = ExtraitChaîne(sInfoBlocage, 1, tab)
      
      // récupère le nom de la machine et le nom de l'utilisateur qui à bloqué la base
      sLogin est une chaine = ExtraitChaîne(sInfoBlocage, 2, tab)
      sMachine est une chaine = ExtraitChaîne(sInfoBlocage, 3, tab)
      
      // si on a des infos sur l'utilisateur et sa machine
      si sLogin <> eot _et_ sMachine <> eot ALORS
      	// mise en forme du libellé
      	LIB_Explications = ChaîneConstruit(<§$000b§>,sNomBase, sLogin, sMachine )
      sinon	
      	// mise en forme du libellé
      	LIB_Explications = <§$000c§>
      FIN
     type : 458752
